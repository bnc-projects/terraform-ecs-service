[
  {
    "cpu": ${cpu_reservation},
    "image": "${docker_image_url}",
    "environment": [
      {
        "name": "JAVA_OPTS",
        "value": "${java_options}"
      },
      {
        "name": "SPRING_PROFILES_ACTIVE",
        "value": "${spring_profile}"
      }
    ],
    "essential": true,
    "healthCheck": {
      "command": [ "CMD-SHELL", "${container_healthcheck}" ],
      "interval": 30,
      "retries": 3,
      "startPeriod": ${healthcheck_grace_period},
      "timeout": 5
    },
    "logConfiguration": {
      "logDriver": "splunk",
      "options": {
        "splunk-format": "raw",
        "splunk-insecureskipverify": "true",
        "splunk-token": "${splunk_token}",
        "splunk-url": "${splunk_url}",
        "splunk-verify-connection": "false"
      }
    },
    "memory": ${memory_limit},
    "memoryReservation": ${memory_reservation},
    "mountPoints": [],
    "name": "${container_name}",
    "portMappings": [
      {
        "containerPort": ${container_port},
        "hostPort": 0,
        "protocol": "tcp"
      }
    ],
    "volumesFrom": []
  }
]